letters = 'АВЕКМНОРСТУХ' # буквы, которые могут быть в номере
numbers = '0123456789'

count = 0 # счетчик
# российский номерной знак: буква - цифра - цифра - цифра - буква - буква, тогда каждый символ берем либо из набора букв, либо из набора цифр
# в соответствии со стандартом
for s1 in letters: # первой, пятой и шестой символ берем из букв
    for s2 in numbers: # второй - четвертый символ из цифр
        for s3 in numbers:
            for s4 in numbers:
                for s5 in letters:
                    for s6 in letters:
                        znak = s1 + s2 + s3 + s4 + s5 + s6 # составляем переменную знака
                        if ((s2 == s3 == s4 or s1 == s5 == s6) or ((s1 == s6 and s1 != s5) and (s2 == s4 and s2 != s3))) and '000' not in znak: # задаем условия, не забывая про 000
                           count += 1 # прибавляем к счетчику
print(count) # 39312
                
