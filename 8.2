s = '01234567' # задем алфавит
chet = '0246' # алфавит из четных
count = 0 # счетчик
# количество переборов зависит от количества цифр в числе
for s1 in s: # перебор первой цифры в числе
    for s2 in s: # второго и тд
        for s3 in s:
            for s4 in s:
                for s5 in s:
                    for s6 in s:
                        for s7 in s:
                            number = s1 + s2 + s3 + s4 + s5 + s6 + s7 # программа выбрала семь цифр, складываем их в число
                            if (s1 > s7) and (s2 < s6) and (s4 in chet) and \
                               (number.count('0') + number.count('2') + number.count('4') + number.count('6') < \
                                number.count('1') + number.count('3') + number.count('5') + number.count('7')) and s1 != '0':
                                # прописываем условие (в данной задаче необязательно указывать, что первая цифра не равна 0, тк 0 меньше любой из цифр среди данных
                                # (не может быть больше последней), но если сказать, что последняя цифра должна быть больше первой, данное условие обязательно
                                count += 1 # если число подходит под поставленные условия, то прибавляем единицу к счетчику
print(count) #67072                   
