s = 'ИКПРЯ' # создаем переменную, содержащую наш алфавит в АЛФАВИТНОМ порядке
count = 0 # счетчик
# количество переборов зависит от количества символов в одном сообщении
for s1 in s: # первый символ нашего сообщения, который берется из алфавита s, символы питоном берутся в указанном нами (алфавитном) порядке
    for s2 in s: # второй и тд
        for s3 in s:
            for s4 in s:
                for s5 in s:
                    word = s1 + s2 + s3 + s4 + s5 # программа выбрала буквы из алфавита, составляем сообщение и задаем переменную
                    count += 1 # с каждым новым словом прибавляем единицу к нашему счетчику, иначе говоря, у каждого сообщения появляется порядковый номер
                    if word == 'ЯКРИП':
                        print(count) # если программа подобрала нужное нам слово, то выводим его порядковый номер
